.ViewportOrientationMarkers
  pointer-events: none // Necessary for click-through to cornerstone element below

  font-size: 15px
  color: rgb(204, 204, 204)
  line-height: 18px

  .orientation-marker
    position: absolute

  .top-mid
    top: 5px
    left: 50%

  .left-mid
    top: 47%
    left: 5px
